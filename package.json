  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:otp": "jest src/modules/otp/otp.controller.spec.ts",
    "test:auth": "jest src/modules/auth/auth.controller.spec.ts",
    "test:questions": "jest src/modules/questions/questions.controller.spec.ts",
    "test:accounts": "jest src/modules/userAccounts/accounts.controller.spec.ts",
    "test:transactions": "jest src/modules/transactions/transactions.controller.spec.ts",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cross-env NODE_ENV=test DATABASE_TEST_URL=postgres://testuser:testpass@localhost:5438/testdb jest --config jest-e2e.json",
    "test:e2e:sanity": "cross-env NODE_ENV=test DATABASE_TEST_URL=postgres://testuser:testpass@localhost:5438/testdb jest --config jest-e2e.json -t '#tra'",
    "prepare": "node -e \"if (process.env.NODE_ENV==='production') process.exit(0);\" || husky install",
    "migration:generate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate -d src/database/data-source.ts",
    "migration:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d src/database/data-source.ts",
    "migration:revert": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert -d src/database/data-source.ts"
  },
